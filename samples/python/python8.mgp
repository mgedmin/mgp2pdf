%left, vgap 2, size 6
def breakpoint(fn):
    def wrapped_fn(*args, **kw):
        import pdb; pdb.set_trace()
        return fn(*args, **kw)
    wrapped_fn.__name__ = fn.__name__
    return wrapped_fn
# ---------------------------------------------------------------------------
%page
%vgap 10


