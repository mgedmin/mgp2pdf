%left, vgap 2, size 6
def debug(prefix):
    def decorator(fn):
        def w_fn(*args, **kw):
            print "%s %s" % (prefix,
                                        fn.__name__)
            return fn(*args, **kw)
        w_fn.__name__ = fn.__name__
        return w_fn
    return decorator
# ---------------------------------------------------------------------------
%page
%vgap 10


%left, vgap 2, size 6
@debug('==>')
def compute(x, y):
    return x + y

>>> compute(2, 3)
==> compute
7
